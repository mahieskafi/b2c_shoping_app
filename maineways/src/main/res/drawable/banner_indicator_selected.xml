<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:width="10dp"
        android:height="10dp">
        <shape
            android:shape="oval">
            <stroke
                android:width="0.5dp"
                android:color="#fc7a1e" />
        </shape>
    </item>

    <item
        android:width="6dp"
        android:height="6dp"
        android:top="2dp"
        android:left="2dp">
        <shape
            android:shape="oval">
            <solid android:color="#fc7a1e"/>
        </shape>
    </item>

</layer-list>