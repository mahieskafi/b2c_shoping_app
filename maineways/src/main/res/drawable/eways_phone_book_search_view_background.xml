<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/selectable_kachel_shape">
        <shape android:shape="rectangle">
            <solid android:color="@color/userphonebook_searchview_background_color"/>
            <corners android:radius="20dp" />
            <stroke
                android:width="@dimen/ewaysphonebook_fragment_searchview_stroke_width"
                android:color="@color/ewaysphonebook_searchview_stroke_color" />

            <padding
                android:bottom="@dimen/userphonebook_searchview_background_paddingbottom"
                android:left="@dimen/userphonebook_searchview_background_paddingleft"
                android:right="@dimen/userphonebook_searchview_background_paddingright"
                android:top="@dimen/userphonebook_searchview_background_paddingtop" />
        </shape>
    </item>
</layer-list>