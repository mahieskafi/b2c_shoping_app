<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="InputElement">
        <attr name="icon" format="reference"/>
        <attr name="font" format="reference"/>
        <attr name="has_cancel_icon" format="boolean" />
        <attr name="android:inputType"/>
    </declare-styleable>


    <declare-styleable name="ButtonElement">
        <attr name="button_icon" format="reference"/>
        <attr name="button_font" format="reference"/>
    </declare-styleable>

    <!--NavigationDrawerItem-->
    <declare-styleable name="NavigationDrawerItem">
        <attr name="android:fontFamily"/>
    </declare-styleable>


    <declare-styleable name="WeiredTabView">
        <attr name="weiredtab_icon1" format="reference" />
        <attr name="weiredtab_icon2" format="reference" />
        <attr name="weiredtab_icon3" format="reference" />
        <attr name="weiredtab_title1" format="string" />
        <attr name="weiredtab_title2" format="string" />
        <attr name="weiredtab_title3" format="string" />
    </declare-styleable>


    <!--ChargeTransactionItem-->
    <declare-styleable name="TransactionItem">
        <attr name="firstFont" format="reference"/>
        <attr name="secondFont" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ShadowLayout">
        <attr name="sl_shadowed" format="boolean"/>
        <attr name="sl_shadow_distance" format="dimension"/>
        <attr name="sl_shadow_angle" format="integer"/>
        <attr name="sl_shadow_radius" format="dimension"/>
        <attr name="sl_shadow_color" format="color"/>
    </declare-styleable>
</resources>